  </DataSources>
  <DataSets>
    <DataSet Name="HotelParameter_DataSet">
      <Query>
        <DataSourceName>HotelParameter_DataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@primaryCategories">
            <Value>=Parameters!primaryCategories.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT fh.hotel_name, dhc.categories, dhc.primaryCategories, du.AlternateUserID, du.username, dl.address, dl.postalCode, dl.city, dl.province, dl.country, dd.Month, dd.MonthName, fh.h_id, fh.Price_per_night, fh.no_of_reserved_rooms, 
                  fh.totalAmount
FROM     FactHotel AS fh INNER JOIN
                  DimDate AS dd ON fh.dateAdded = dd.DateKey INNER JOIN
                  DimHotelCategory AS dhc ON fh.HotelCategoryKey = dhc.HotelCategorySK INNER JOIN
                  DimUser AS du ON fh.UserKey = du.userSK INNER JOIN
                  DimLocation AS dl ON fh.LocationKey = dl.LocationSK
WHERE  (dhc.primaryCategories = @primaryCategories)</CommandText>
      </Query>
